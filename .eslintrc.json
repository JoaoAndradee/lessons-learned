{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/prop-types": "off",
    "react/react-in-jsx-scope": "off",
    "space-before-function-paren": "off",
    "no-console": "warn",
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "jsx-quotes": [
      "error",
      "prefer-double"
    ]
  }
}
